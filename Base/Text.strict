has Characters
from(number)
	Text(45) is "45"
	for digits(number)
		Character(value)
digits(number) Numbers
	digits(1) is (1)
	digits(123) is (1, 2, 3)
	floor(number / 10) is 0 ? (number % 10) else digits(floor(number / 10)) + number % 10
+(other) Text
	+("more") is "more"
	"Hey" + " " + "you" is "Hey you"
	Characters + other.Characters